{
  "name": "agentbasis",
  "version": "0.1.0",
  "description": "AgentBasis TypeScript SDK - Observability and monitoring for AI agents",
  "author": "AgentBasis",
  "license": "MIT",
  "keywords": [
    "ai",
    "agents",
    "llm",
    "observability",
    "monitoring",
    "tracing",
    "opentelemetry",
    "otel",
    "openai",
    "anthropic",
    "gemini",
    "langchain",
    "vercel-ai",
    "mastra"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/agentbasis/agentbasis-ts-sdk"
  },
  "homepage": "https://agentbasis.co",
  "bugs": {
    "url": "https://github.com/agentbasis/agentbasis-ts-sdk/issues"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./llms/openai": {
      "import": {
        "types": "./dist/llms/openai/index.d.ts",
        "default": "./dist/llms/openai/index.js"
      },
      "require": {
        "types": "./dist/llms/openai/index.d.cts",
        "default": "./dist/llms/openai/index.cjs"
      }
    },
    "./llms/anthropic": {
      "import": {
        "types": "./dist/llms/anthropic/index.d.ts",
        "default": "./dist/llms/anthropic/index.js"
      },
      "require": {
        "types": "./dist/llms/anthropic/index.d.cts",
        "default": "./dist/llms/anthropic/index.cjs"
      }
    },
    "./llms/gemini": {
      "import": {
        "types": "./dist/llms/gemini/index.d.ts",
        "default": "./dist/llms/gemini/index.js"
      },
      "require": {
        "types": "./dist/llms/gemini/index.d.cts",
        "default": "./dist/llms/gemini/index.cjs"
      }
    },
    "./frameworks/langchain": {
      "import": {
        "types": "./dist/frameworks/langchain/index.d.ts",
        "default": "./dist/frameworks/langchain/index.js"
      },
      "require": {
        "types": "./dist/frameworks/langchain/index.d.cts",
        "default": "./dist/frameworks/langchain/index.cjs"
      }
    },
    "./frameworks/vercel-ai": {
      "import": {
        "types": "./dist/frameworks/vercel-ai-sdk/index.d.ts",
        "default": "./dist/frameworks/vercel-ai-sdk/index.js"
      },
      "require": {
        "types": "./dist/frameworks/vercel-ai-sdk/index.d.cts",
        "default": "./dist/frameworks/vercel-ai-sdk/index.cjs"
      }
    },
    "./frameworks/mastra": {
      "import": {
        "types": "./dist/frameworks/mastra/index.d.ts",
        "default": "./dist/frameworks/mastra/index.js"
      },
      "require": {
        "types": "./dist/frameworks/mastra/index.d.cts",
        "default": "./dist/frameworks/mastra/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.48.0",
    "@opentelemetry/resources": "^1.21.0",
    "@opentelemetry/sdk-trace-base": "^1.21.0",
    "@opentelemetry/semantic-conventions": "^1.21.0"
  },
  "peerDependencies": {
    "@anthropic-ai/sdk": ">=0.20.0",
    "@google/generative-ai": ">=0.10.0",
    "@langchain/core": ">=0.2.0",
    "ai": ">=3.0.0",
    "openai": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "openai": {
      "optional": true
    },
    "@anthropic-ai/sdk": {
      "optional": true
    },
    "@google/generative-ai": {
      "optional": true
    },
    "@langchain/core": {
      "optional": true
    },
    "ai": {
      "optional": true
    }
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "@google/generative-ai": "^0.10.0",
    "@langchain/core": "^1.1.17",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/coverage-v8": "^4.0.18",
    "ai": "^6.0.49",
    "eslint": "^8.56.0",
    "openai": "^4.28.0",
    "prettier": "^3.2.4",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^4.0.18"
  }
}
